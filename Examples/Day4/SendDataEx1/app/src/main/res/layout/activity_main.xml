<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.administrator.senddataex1.MainActivity">
    <Button
        android:id="@+id/btn1"
        android:onClick="sendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="새로운 액티비티로 데이터 보내기" />
    <Button
        android:id="@+id/btn2"
        android:onClick="sendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="새로운 액티비티에서 데이터 받기" />
    <Button
        android:id="@+id/btn3"
        android:onClick="sendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="새로운 액티비티로 데이터 보내고받기1" />
    <Button
        android:id="@+id/btn4"
        android:onClick="sendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="새로운 액티비티로 데이터 보내고받기2" />
    <Button
        android:id="@+id/btn5"
        android:onClick="sendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="새로운 액티비티로 객체 데이터 보내기1" />
    <Button
        android:id="@+id/btn6"
        android:onClick="sendData"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="새로운 액티비티로 객체 데이터 보내기2" />
</LinearLayout>
